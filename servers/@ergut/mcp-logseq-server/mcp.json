{
  "id": "@ergut/mcp-logseq-server",
  "title": "Mcp-Logseq-Server",
  "description": "An MCP server to interact with LogSeq via its API, enabling operations like page management and content search.",
  "categories": ["API Management", "Content Management System"],
  "tags": [
    "LogSeq",
    "API",
    "MCP"
  ],
  "creator": "ergut",
  "logoUrl": "",
  "publishDate": "2024-12-31T07:10:20Z",
  "rating": 5,
  "sources": {
    "github": "https://github.com/ergut/mcp-logseq-server"
  },
  "type": "stdio",
  "commandInfo": {
    "command": "uvx",
    "args": [
      "mcp-logseq"
    ],
    "env": {
      "LOGSEQ_API_TOKEN": "**LOGSEQ_API_TOKEN**",
      "LOGSEQ_API_URL": "**LOGSEQ_API_URL**"
    }
  },
  "defVersion": "1",
  "parameters": {
    "LOGSEQ_API_TOKEN": {
      "type": "string",
      "required": true,
      "description": "API token for authenticating with the LogSeq API."
    },
    "LOGSEQ_API_URL": {
      "type": "string",
      "required": true,
      "description": "The base URL for the LogSeq API."
    }
  }
}
