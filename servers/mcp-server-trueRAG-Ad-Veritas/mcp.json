{
  "id": "mcp-server-trueRAG-Ad-Veritas",
  "title": "Mcp-Server-Truerag",
  "description": "This repository contains an MCP server implementation for a GraphQL API providing access to TrueRAG policy systems.",
  "tags": [
    "MCP",
    "GraphQL",
    "Python"
  ],
  "creator": "Ad-Veritas",
  "logoUrl": "",
  "publishDate": "2024-12-31T06:56:37Z",
  "rating": 5,
  "sources": {
    "github": "https://github.com/Ad-Veritas/mcp-server-trueRAG"
  },
  "type": "stdio",
  "commandInfo": {
    "command": "uv",
    "args": [
      "--directory",
      "**PATH_TO_SERVER**",
      "run",
      "fastmcp",
      "run",
      "server.py"
    ],
    "env": {
      "GRAPHQL_API_KEY": "**GRAPHQL_API_KEY**",
      "GRAPHQL_ENDPOINT": "**GRAPHQL_ENDPOINT**"
    }
  },
  "defVersion": "1",
  "parameters": {
    "GRAPHQL_API_KEY": {
      "type": "string",
      "required": true,
      "description": "The API key for authenticating against the GraphQL API."
    },
    "GRAPHQL_ENDPOINT": {
      "type": "string",
      "required": true,
      "description": "The GraphQL API endpoint URL."
    },
    "PATH_TO_SERVER": {
      "type": "string",
      "required": true,
      "description": "The path to the directory of the MCP server implementation."
    }
  }
}